  },
  "dependencies": {
    "@fieldview/data-model": "workspace:*",
    "@prisma/client": "^6.0.0",
    "express": "^4.18.2",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^1.0.4",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
